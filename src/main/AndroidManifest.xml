        </activity>

        <activity
            android:name=".Sport"
            android:label="@string/sport" >
        </activity>
        <activity
            android:name=".Computer"
            android:label="@string/computer_science" >
        </activity>
        <activity
            android:name=".General"
            android:label="@string/activity_general_science" >
        </activity>
        <activity
            android:name=".Select"
            android:label="@string/activity_select" >
        </activity>
        <activity
            android:name=".Congratulations"
            android:label="@string/congrats" >
        </activity>

    </application>

</manifest>